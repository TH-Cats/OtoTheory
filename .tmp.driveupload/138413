var __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$next$2f$dist$2f$server$2f$route$2d$modules$2f$app$2d$page$2f$vendored$2f$ssr$2f$react$2d$jsx$2d$dev$2d$runtime$2e$js__$5b$app$2d$ssr$5d$__$28$ecmascript$29$__ = __turbopack_context__.i("[project]/node_modules/next/dist/server/route-modules/app-page/vendored/ssr/react-jsx-dev-runtime.js [app-ssr] (ecmascript)");
;
const PAD = {
    left: 28,
    right: 16,
    top: 16,
    bottom: 32
};
// Note names in chromatic order (♭-preferred notation)
const NOTE_NAMES = [
    'C',
    'C#',
    'D',
    'Eb',
    'E',
    'F',
    'F#',
    'G',
    'Ab',
    'A',
    'Bb',
    'B'
];
const NOTE_INDEX = {
    'C': 0,
    'C#': 1,
    'D': 2,
    'D#': 3,
    'Eb': 3,
    'E': 4,
    'F': 5,
    'F#': 6,
    'G': 7,
    'G#': 8,
    'Ab': 8,
    'A': 9,
    'A#': 10,
    'Bb': 10,
    'B': 11
};
// Open strings (1st to 6th: high E to low E)
// Array index 0 = 1st string (high E), index 5 = 6th string (low E)
const OPEN_STRINGS = [
    'E',
    'B',
    'G',
    'D',
    'A',
    'E'
];
// Get note name for a fret on a string
function getNoteName(stringIdx, fret) {
    const openNote = OPEN_STRINGS[stringIdx];
    const openIdx = NOTE_INDEX[openNote];
    const noteIdx = (openIdx + fret) % 12;
    return NOTE_NAMES[noteIdx];
}
// Get Roman numeral for a note relative to root (R = Root instead of I)
function getRoman(noteName, root) {
    const rootIdx = NOTE_INDEX[root];
    const noteIdx = NOTE_INDEX[noteName];
    const interval = (noteIdx - rootIdx + 12) % 12;
    const romans = [
        'R',
        '♭II',
        'II',
        '♭III',
        'III',
        'IV',
        '♭V',
        'V',
        '#V',
        'VI',
        '♭VII',
        'VII'
    ];
    return romans[interval];
}
// Check if a note is the root note
function isRootNote(stringIdx, fret, root) {
    if (fret === 'x' || fret === 0) return false;
    const noteName = getNoteName(stringIdx, fret);
    return noteName === root;
}
function ChordDiagram({ frets, fingers, barres = [], root, displayMode, width = 240, maxFrets = 4 }) {
    const height = 160;
    const innerW = width - PAD.left - PAD.right;
    const innerH = height - PAD.top - PAD.bottom;
    const numStrings = 6;
    const fretW = innerW / maxFrets;
    const stringH = innerH / (numStrings - 1);
    const pos = frets.filter((f)=>typeof f === 'number' && f > 0);
    const baseFret = pos.length ? Math.min(...pos) : 1;
    const showNut = baseFret === 1;
    // Horizontal layout: frets go left to right
    const xForFret = (abs)=>{
        if (abs === 0) return PAD.left - 12;
        const rel = abs - baseFret + 1;
        return PAD.left + fretW * (rel - 0.5);
    };
    // Strings go top to bottom (1st string at top, 6th string at bottom)
    const yForString = (sIdx)=>PAD.top + stringH * sIdx;
    const grid = [];
    // Draw strings (horizontal lines)
    for(let s = 0; s < numStrings; s++){
        grid.push(/*#__PURE__*/ (0, __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$next$2f$dist$2f$server$2f$route$2d$modules$2f$app$2d$page$2f$vendored$2f$ssr$2f$react$2d$jsx$2d$dev$2d$runtime$2e$js__$5b$app$2d$ssr$5d$__$28$ecmascript$29$__["jsxDEV"])("line", {
            x1: PAD.left,
            y1: yForString(s),
            x2: PAD.left + innerW,
            y2: yForString(s),
            stroke: "#575a5f",
            strokeWidth: 1
        }, `s${s}`, false, {
            fileName: "[project]/src/components/chords/ChordDiagram.tsx",
            lineNumber: 77,
            columnNumber: 15
        }, this));
    }
    // Draw frets (vertical lines)
    for(let f = 1; f <= maxFrets; f++){
        grid.push(/*#__PURE__*/ (0, __TURBOPACK__imported__module__$5b$project$5d2f$node_modules$2f$next$2f$dist$2f$server$2f$route$2d$modules$2f$app$2d$page$2f$vendored$2f$ssr$2f$react$2d$jsx$2d$dev$2d$runtime$2e$js__$5b$app$2d$ssr$5d$__$28$ecmascript$29$__["jsxDEV"])("line", {
            x1: PAD.left + fretW * f,
            y1: PAD.top,
