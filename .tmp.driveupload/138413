function getStaticChord(symbol) {
    return ALL_STATIC_CHORDS.find((c)=>c.symbol === symbol);
}
}),
"[project]/src/components/chords/ChordDiagram.tsx [app-ssr] (ecmascript)", ((__turbopack_context__) => {
"use strict";

