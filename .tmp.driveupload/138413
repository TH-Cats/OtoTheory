// Compact shapes for each quality (top 3-4 strings only)
function shapeCompactMajor(s) {
    const frets = [
        'x',
        'x',
        s + 2,
        s + 2,
        s + 2,
        s
    ];
    const fingers = [
        null,
        null,
        2,
        3,
        4,
        1
    ];
    return {
        id: `compact-major-${s}`,
        label: 'Compact (Top 4)',
        frets,
        fingers,
        tips: [
            'Bright highs, clear voicing.',
            'Mini form that won\'t muddy vocals.',
            'Mute 5th & 6th strings.'
        ]
    };
}
