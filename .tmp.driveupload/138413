// Helper to convert static form to old ChordShape format
function convertToChordShape(form) {
    return {
        id: form.id,
        label: form.shapeName || 'Unknown',
        frets: form.frets,
        fingers: form.fingers,
        barres: form.barres,
        tips: form.tips
    };
}
