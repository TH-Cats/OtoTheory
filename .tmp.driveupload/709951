eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   \"default\": () => (/* binding */ ArticleRenderer)\n/* harmony export */ });\n/* harmony import */ var react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! react/jsx-dev-runtime */ \"(ssr)/./node_modules/next/dist/server/future/route-modules/app-page/vendored/ssr/react-jsx-dev-runtime.js\");\n/* harmony import */ var react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0___default = /*#__PURE__*/__webpack_require__.n(react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0__);\n/* harmony import */ var _FACTCard__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ./FACTCard */ \"(ssr)/./src/app/resources/learn/_components/FACTCard.tsx\");\n/**\n * @fileoverview Article Renderer - Markdown to HTML conversion\n * \n * SSOT参照:\n * - メイン仕様: /docs/SSOT/v3.2_SSOT.md\n * - 実装仕様: /docs/SSOT/v3.2_Implementation_SSOT.md\n * - リソース仕様: /docs/SSOT/RESOURCES_SSOT_v1.md\n * \n * 変更時は必ずSSOTとの整合性を確認すること\n */ /* __next_internal_client_entry_do_not_use__ default auto */ \n\nfunction ArticleRenderer({ article }) {\n    // Simple markdown to HTML conversion with custom blocks\n    const convertMarkdownToHtml = (markdown)=>{\n        let html = markdown;\n        // Process FACT cards\n        const factCardRegex = /:::fact source=\"([^\"]+)\"\\n([\\s\\S]*?)\\n:::/g;\n        html = html.replace(factCardRegex, (match, source, content)=>{\n            return `<div class=\"fact-card\" data-source=\"${source}\" data-content=\"${content.trim()}\"></div>`;\n        });\n        // Headers\n        html = html.replace(/^### (.*)$/gm, '<h3 class=\"text-lg font-semibold mt-6 mb-3\">$1</h3>');\n        html = html.replace(/^## (.*)$/gm, '<h2 class=\"text-2xl font-bold mt-8 mb-4 border-b pb-2\">$1</h2>');\n        html = html.replace(/^# (.*)$/gm, '<h1 class=\"text-3xl font-bold mb-6\">$1</h1>');\n        // Bold, italic, inline code\n        html = html.replace(/\\*\\*(.*?)\\*\\*/g, '<strong class=\"font-semibold\">$1</strong>');\n        html = html.replace(/\\*(.*?)\\*/g, \"<em>$1</em>\");\n        html = html.replace(/`(.*?)`/g, '<code class=\"px-1.5 py-0.5 bg-gray-100 dark:bg-gray-800 rounded text-sm font-mono\">$1</code>');\n        // Links\n        html = html.replace(/\\[([^\\]]+)\\]\\(([^\\)]+)\\)/g, '<a href=\"$2\" class=\"text-blue-600 dark:text-blue-400 hover:underline\">$1</a>');\n        // Process line by line for lists and tables\n        const lines = html.split(\"\\n\");\n        const processed = [];\n        let inList = false;\n        let inTable = false;\n        for(let i = 0; i < lines.length; i++){\n            const line = lines[i];\n            // Table detection\n            if (line.trim().startsWith(\"|\") && line.trim().endsWith(\"|\")) {\n                if (!inTable) {\n                    inTable = true;\n                    const cells = line.split(\"|\").filter((s)=>s.trim());\n                    processed.push('<table class=\"w-full border-collapse my-4 text-sm\"><thead><tr>' + cells.map((cell)=>`<th class=\"border border-gray-300 dark:border-gray-600 px-3 py-2 bg-gray-100 dark:bg-gray-800 text-left\">${cell.trim()}</th>`).join(\"\") + \"</tr></thead><tbody>\");\n                    i++; // Skip separator line\n                } else {\n                    const cells = line.split(\"|\").filter((s)=>s.trim());\n                    processed.push(\"<tr>\" + cells.map((cell)=>`<td class=\"border border-gray-300 dark:border-gray-600 px-3 py-2\">${cell.trim()}</td>`).join(\"\") + \"</tr>\");\n                }\n                // Check if next line is not a table row\n                if (!lines[i + 1] || !lines[i + 1].trim().startsWith(\"|\")) {\n                    processed.push(\"</tbody></table>\");\n                    inTable = false;\n                }\n                continue;\n            }\n            // List items\n            if (line.match(/^- /)) {\n                if (!inList) {\n                    processed.push('<ul class=\"list-disc pl-6 space-y-1 mb-4\">');\n                    inList = true;\n                }\n                processed.push(`<li>${line.substring(2)}</li>`);\n            } else {\n                if (inList) {\n                    processed.push(\"</ul>\");\n                    inList = false;\n                }\n                // Horizontal rule\n                if (line.trim() === \"---\") {\n                    processed.push('<hr class=\"my-8 border-gray-300 dark:border-gray-600\" />');\n                } else if (line.trim() === \"\" || line.startsWith(\"<\")) {\n                    processed.push(line);\n                } else if (line.trim() && !line.startsWith(\"#\") && !line.startsWith(\">\")) {\n                    processed.push(`<p class=\"mb-4 leading-relaxed\">${line}</p>`);\n                } else {\n                    processed.push(line);\n                }\n            }\n        }\n        if (inList) {\n            processed.push(\"</ul>\");\n        }\n        return processed.join(\"\\n\");\n    };\n    const htmlContent = convertMarkdownToHtml(article.content);\n    // Extract FACT cards from HTML\n    const factCards = [];\n    const factCardRegex = /<div class=\"fact-card\" data-source=\"([^\"]+)\" data-content=\"([^\"]+)\"><\\/div>/g;\n    let match;\n    while((match = factCardRegex.exec(htmlContent)) !== null){\n        factCards.push({\n            source: match[1],\n            content: match[2]\n        });\n    }\n    // Replace FACT card placeholders with actual components\n    let processedHtml = htmlContent;\n    factCards.forEach((card, index)=>{\n        const placeholder = `<div class=\"fact-card\" data-source=\"${card.source}\" data-content=\"${card.content}\"></div>`;\n        const componentId = `fact-card-${index}`;\n        processedHtml = processedHtml.replace(placeholder, `<div id=\"${componentId}\"></div>`);\n    });\n    return /*#__PURE__*/ (0,react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0__.jsxDEV)(\"div\", {\n        className: \"prose prose-sm sm:prose-base max-w-none dark:prose-invert prose-headings:scroll-mt-20 prose-a:no-underline hover:prose-a:underline\",\n        children: [\n            /*#__PURE__*/ (0,react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0__.jsxDEV)(\"div\", {\n                dangerouslySetInnerHTML: {\n                    __html: processedHtml\n                }\n            }, void 0, false, {\n                fileName: \"/Users/nh/App/OtoTheory/ototheory-web/src/app/resources/learn/_components/ArticleRenderer.tsx\",\n                lineNumber: 138,\n                columnNumber: 7\n            }, this),\n            factCards.map((card, index)=>/*#__PURE__*/ (0,react_jsx_dev_runtime__WEBPACK_IMPORTED_MODULE_0__.jsxDEV)(_FACTCard__WEBPACK_IMPORTED_MODULE_1__[\"default\"], {\n                    source: card.source,\n                    children: card.content\n                }, index, false, {\n                    fileName: \"/Users/nh/App/OtoTheory/ototheory-web/src/app/resources/learn/_components/ArticleRenderer.tsx\",\n                    lineNumber: 142,\n                    columnNumber: 9\n                }, this))\n        ]\n    }, void 0, true, {\n        fileName: \"/Users/nh/App/OtoTheory/ototheory-web/src/app/resources/learn/_components/ArticleRenderer.tsx\",\n        lineNumber: 137,\n        columnNumber: 5\n    }, this);\n}\n//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///(ssr)/./src/app/resources/learn/_components/ArticleRenderer.tsx\n");

