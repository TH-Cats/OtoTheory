eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   player: () => (/* binding */ player)\n/* harmony export */ });\n/* harmony import */ var _telemetry__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! ../telemetry */ \"(ssr)/./src/lib/telemetry.ts\");\n/* harmony import */ var _voices__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ./voices */ \"(ssr)/./src/lib/audio/voices.ts\");\n/* harmony import */ var soundfont_player__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! soundfont-player */ \"(ssr)/./node_modules/soundfont-player/lib/index.js\");\n/* harmony import */ var soundfont_player__WEBPACK_IMPORTED_MODULE_2___default = /*#__PURE__*/__webpack_require__.n(soundfont_player__WEBPACK_IMPORTED_MODULE_2__);\n/* eslint-disable @typescript-eslint/no-explicit-any */ \n\n\nconst isBrowser = \"undefined\" !== \"undefined\";\nfunction midiToHz(midi) {\n    return 440 * Math.pow(2, (midi - 69) / 12);\n}\nfunction createNoop() {\n    const s = {\n        enabled: false,\n        volume: 0.8\n    };\n    return {\n        async resume () {},\n        async toggleEnabled () {},\n        setVolume () {},\n        getState () {\n            return s;\n        },\n        subscribe () {\n            return ()=>{};\n        },\n        playNote () {},\n        playChord () {},\n        async setInstrument () {},\n        getInstrument () {\n            return \"acoustic_guitar_steel\";\n        }\n    };\n}\nconst player = isBrowser ? (()=>{\n    // lazy context / graph\n    let ctx = null;\n    let master = null;\n    let sfGuitar = null;\n    const VALID_INSTR = new Set([\n        \"acoustic_guitar_nylon\",\n        \"acoustic_guitar_steel\",\n        \"electric_guitar_clean\",\n        \"electric_guitar_jazz\",\n        \"electric_guitar_muted\",\n        \"overdriven_guitar\",\n        \"distortion_guitar\",\n        \"acoustic_grand_piano\"\n    ]);\n    let currentIn