f (hasV7 && (type === \"HarmonicMinor\" || type === \"MelodicMinor\")) bonus += 0.03;\n        const raw = Math.min(1, cover + bonus);\n        const score = Math.round(raw * 100);\n        return {\n            root: key.root,\n            type,\n            score\n        };\n    }).sort((a, b)=>b.score - a.score);\n}\n//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///(ssr)/./src/lib/analysis/scoreScales.ts\n");

