tion dedupe(xs) {\n    const res = [];\n    for (const m of xs){\n        if (res.some((r)=>!(m.end <= r.start || r.end <= m.start))) continue;\n        res.push(m);\n    }\n    return res;\n}\n//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///(ssr)/./src/lib/theory/roman/match.ts\n");

