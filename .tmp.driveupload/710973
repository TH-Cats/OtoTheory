) {\n                    chordIdx = 0; // Loop\n                }\n                const { chord, index } = filledSlots[chordIdx];\n                setCurrentChordIndex(index);\n                await playChordSymbol(chord);\n                chordIdx++;\n            } else {\n                // Re-trigger the same chord\n                if (chordIdx > 0 && chordIdx <= filledSlots.length) {\n                    const { chord } = filledSlots[chordIdx - 1];\n                    await playChordSymbol(chord);\n                }\n            }\n            beatCount++;\n        };\n        if (!isPlayingRef.current) return;\n        await playBeat(); // Play first beat immediately\n        if (!isPlayingRef.current) return;\n        playbackTimerRef.current = setInterval(playBeat, beatDuration);\n        (0,_lib_telemetry__WEBPACK_IMPORTED_MODULE_25__.track)(\"progression_play\", {\n            page: \"chord_progression\",\n            bpm,\n            chordCount: filledSlots.length\n        });\n    }, [\n        slots,\n        bpm,\n        stopPlayback\n    ]);\n    const togglePlayback = (0,react__WEBPACK_IMPORTED_MODULE_1__.useCallback)(()=>{\n        if (isPlaying) {\n            stopPlayback();\n        } else {\n            startPlayback();\n        }\n    }, [\n        isPlaying,\n        startPlayback,\n        stopPlayback\n    ]);\n    // Stop playback when component unmounts or slots change\n    (0,react__WEBPACK_IMPORTED_MODULE_1__.useEffect)(()=>{\n        return ()=>{\n            if (playbackTimerRef.current) {\n                clearInterval(playbackTimerRef.current);\n            }\n        };\n    }, []);\n    (0,react__WEBPACK_IMPORTED_MODULE_1__.useEffect)(()=>{\n        if (isPlaying) {\n            stopPlayback();\n        }\n    }, [\n        slots\n    ]);\n    const removeSlot = (0,react__WEBPACK_IMPORTED_MODULE_1__.useCallback)((idx)=>{\n        setSlots((prev)=>{\n            const arr = prev.slice();\n            arr[idx] = \"\";\n           