/***/ ((__unused_webpack_module, __webpack_exports__, __webpack_require__) => {

"use strict";
eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   progressionToRoman: () => (/* binding */ progressionToRoman),\n/* harmony export */   romanizeChord: () => (/* binding */ romanizeChord)\n/* harmony export */ });\n/* harmony import */ var _lib_scales__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @/lib/scales */ \"(ssr)/./src/lib/scales.ts\");\n/* harmony import */ var _lib_theory__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @/lib/theory */ \"(ssr)/./src/lib/theory.ts\");\n\n\nconst ROMAN_UPPER = [\n    \"I\",\n    \"II\",\n    \"III\",\n    \"IV\",\n    \"V\",\n    \"VI\",\n    \"VII\"\n];\nfunction baseRomanFromDegreeLabel(lbl) {\n    if (!lbl) return \"I\";\n    const n = Number(lbl.replace(/[♭#]/g, \"\"));\n    const idx = isFinite(n) ? Math.max(1, Math.min(7, n)) - 1 : 0;\n    return ROMAN_UPPER[idx];\n}\nfunction qualitySuffix(qual) {\n    return /7/.test(qual) ? \"7\" : \"\";\n}\nfunction diminishedSymbol(qual) {\n    return /dim|o/.test(qual) ? \"\\xb0\" : \"\";\n}\nfunction romanizeChord(chord, tonicPc, scaleType) {\n    const { root, qual } = (0,_lib_theory__WEBPACK_IMPORTED_MODULE_1__.parseChord)(chord);\n    const effectiveType = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.parentModeOf)(scaleType) ?? scaleType;\n    // try exact degree label first\n    let lbl = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.degreeLabelFor)(root, tonicPc, effectiveType);\n    let accidental = lbl?.startsWith(\"♭\") ? \"♭\" : lbl?.startsWith(\"#\") ? \"♯\" : \"\";\n    // if null, try ±1 semitone nearest mapping\n    if (!lbl) {\n        const pcs = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.getScalePitches)(tonicPc, effectiveType); // absolute pcs\n        const minus = (root + 12 - 1) % 12;\n        const plus = (root + 1) % 12;\n        const idxMinus = pcs.indexOf(minus);\n        const idxPlus = pcs.indexOf(plus);\n        if (idxMinus !== -1) {\n            lbl = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.degreeLabelFor)(minus, tonicPc, effectiveType); // e.g., \"3\"\n            accidental = \"♭\";\n        } else if (idxPlus !== -1) {\n            lbl = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.degreeLabelFor)(plus, tonicPc, effectiveType);\n            accidental = \"♯\";\n        }\n    }\n    const base = baseRomanFromDegreeLabel(lbl);\n    const sym = diminishedSymbol(qual);\n    const suf = qualitySuffix(qual);\n    return `${accidental}${base}${sym}${suf}`;\n}\nfunction progressionToRoman(chords, tonicPc, scaleType) {\n    return chords.map((c)=>romanizeChord(c, tonicPc, scaleType));\n}\n//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///(ssr)/./src/lib/roman.ts\n");

/***/ }),

