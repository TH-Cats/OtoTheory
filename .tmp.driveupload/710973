eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   DEFAULT_MIDI_OCTAVE: () => (/* binding */ DEFAULT_MIDI_OCTAVE),\n/* harmony export */   PC_NAMES: () => (/* binding */ PC_NAMES),\n/* harmony export */   PITCH_CLASS_COUNT: () => (/* binding */ PITCH_CLASS_COUNT),\n/* harmony export */   mod12: () => (/* binding */ mod12),\n/* harmony export */   nameToPc: () => (/* binding */ nameToPc),\n/* harmony export */   pcToName: () => (/* binding */ pcToName)\n/* harmony export */ });\n/**\n * \n */ /** 0=C, 1=C#, ..., 11=B */ const PC_NAMES = [\n    \"C\",\n    \"C#\",\n    \"D\",\n    \"Eb\",\n    \"E\",\n    \"F\",\n    \"F#\",\n    \"G\",\n    \"Ab\",\n    \"A\",\n    \"Bb\",\n    \"B\"\n];\n/**  */ const PITCH_CLASS_COUNT = 12;\n/** MIDIC4 = MIDI 60 */ const DEFAULT_MIDI_OCTAVE = 60;\n/**\n * 0-11\n */ function mod12(pc) {\n    return (pc % 12 + 12) % 12;\n}\n/**\n * \n */ function pcToName(pc) {\n    return PC_NAMES[mod12(pc)];\n}\n/**\n * \n */ function nameToPc(name) {\n    const index = PC_NAMES.indexOf(name);\n    return index >= 0 ? index : -1;\n}\n//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///(ssr)/./src/lib/music/constants.ts\n");

