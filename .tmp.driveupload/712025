eval(__webpack_require__.ts("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   progressionToRoman: function() { return /* binding */ progressionToRoman; },\n/* harmony export */   romanizeChord: function() { return /* binding */ romanizeChord; }\n/* harmony export */ });\n/* harmony import */ var _lib_scales__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @/lib/scales */ \"(app-pages-browser)/./src/lib/scales.ts\");\n/* harmony import */ var _lib_theory__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @/lib/theory */ \"(app-pages-browser)/./src/lib/theory.ts\");\n\n\nconst ROMAN_UPPER = [\n    \"I\",\n    \"II\",\n    \"III\",\n    \"IV\",\n    \"V\",\n    \"VI\",\n    \"VII\"\n];\nfunction baseRomanFromDegreeLabel(lbl) {\n    if (!lbl) return \"I\";\n    const n = Number(lbl.replace(/[#]/g, \"\"));\n    const idx = isFinite(n) ? Math.max(1, Math.min(7, n)) - 1 : 0;\n    return ROMAN_UPPER[idx];\n}\nfunction qualitySuffix(qual) {\n    return /7/.test(qual) ? \"7\" : \"\";\n}\nfunction diminishedSymbol(qual) {\n    return /dim|o/.test(qual) ? \"\\xb0\" : \"\";\n}\nfunction romanizeChord(chord, tonicPc, scaleType) {\n    const { root, qual } = (0,_lib_theory__WEBPACK_IMPORTED_MODULE_1__.parseChord)(chord);\n    var _parentModeOf;\n    const effectiveType = (_parentModeOf = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.parentModeOf)(scaleType)) !== null && _parentModeOf !== void 0 ? _parentModeOf : scaleType;\n    // try exact degree label first\n    let lbl = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.degreeLabelFor)(root, tonicPc, effectiveType);\n    let accidental = (lbl === null || lbl === void 0 ? void 0 : lbl.startsWith(\"\")) ? \"\" : (lbl === null || lbl === void 0 ? void 0 : lbl.startsWith(\"#\")) ? \"\" : \"\";\n    // if null, try 1 semitone nearest mapping\n    if (!lbl) {\n        const pcs = (0,_lib_scales__WEBPACK_IMPORTED_MODULE_0__.getScalePitches)(tonicPc