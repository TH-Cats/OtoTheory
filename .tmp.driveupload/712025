\"7alt\": \"7alt\",\n    \"7altered\": \"7alt\"\n};\n// Quality resolver: QUALITY_ALIASESを通して正規化\nfunction resolveQuality(qual) {\n    var _QUALITY_ALIASES_qual;\n    return (_QUALITY_ALIASES_qual = QUALITY_ALIASES[qual]) !== null && _QUALITY_ALIASES_qual !== void 0 ? _QUALITY_ALIASES_qual : qual;\n}\n// QUALITY_TONES: Chord Libraryの全40+品質を網羅\nconst QUALITY_TONES = {\n    // Basic triads (Core)\n    M: [\n        0,\n        4,\n        7\n    ],\n    m: [\n        0,\n        3,\n        7\n    ],\n    aug: [\n        0,\n        4,\n        8\n    ],\n    dim: [\n        0,\n        3,\n        6\n    ],\n    // Suspended (Core)\n    sus2: [\n        0,\n        2,\n        7\n    ],\n    sus4: [\n        0,\n        5,\n        7\n    ],\n    // Sixth chords (Core)\n    \"6\": [\n        0,\n        4,\n        7,\n        9\n    ],\n    m6: [\n        0,\n        3,\n        7,\n        9\n    ],\n    \"6/9\": [\n        0,\n        4,\n        7,\n        9,\n        14\n    ],\n    // Seventh chords (Core)\n    \"7\": [\n        0,\n        4,\n        7,\n        10\n    ],\n    M7: [\n        0,\n        4,\n        7,\n        11\n    ],\n    m7: [\n        0,\n        3,\n        7,\n        10\n    ],\n    dim7: [\n        0,\n        3,\n        6,\n        9\n    ],\n    m7b5: [\n        0,\n        3,\n        6,\n        10\n    ],\n    mMaj7: [\n        0,\n        3,\n        7,\n        11\n    ],\n    // Ninth chords (Core)\n    \"9\": [\n        0,\n        4,\n        7,\n        10,\n        14\n    ],\n    M9: [\n        0,\n        4,\n        7,\n        11,\n        14\n    ],\n    m9: [\n        0,\n        3,\n        7,\n        10,\n        14\n    ],\n    add9: [\n        0,\n        4,\n        7,\n        14\n    ],\n    madd9: [\n        0,\n        3,\n        7,\n        14\n    ],\n    // Extended chords (Advanced)\n    \"11\": [\n        0,\n        4,\n        7,\n        10,\n        14,\n        17\n    ],\n    M11: [\n      