"use strict";
eval(__webpack_require__.ts("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   DEFAULT_MIDI_OCTAVE: function() { return /* binding */ DEFAULT_MIDI_OCTAVE; },\n/* harmony export */   PC_NAMES: function() { return /* binding */ PC_NAMES; },\n/* harmony export */   PITCH_CLASS_COUNT: function() { return /* binding */ PITCH_CLASS_COUNT; },\n/* harmony export */   mod12: function() { return /* binding */ mod12; },\n/* harmony export */   nameToPc: function() { return /* binding */ nameToPc; },\n/* harmony export */   pcToName: function() { return /* binding */ pcToName; }\n/* harmony export */ });\n/**\n * 音楽理論の共通定数\n */ /** ピッチクラスの名前（0=C, 1=C#, ..., 11=B） */ const PC_NAMES = [\n    \"C\",\n    \"C#\",\n    \"D\",\n    \"Eb\",\n    \"E\",\n    \"F\",\n    \"F#\",\n    \"G\",\n    \"Ab\",\n    \"A\",\n    \"Bb\",\n    \"B\"\n];\n/** ピッチクラスの総数 */ const PITCH_CLASS_COUNT = 12;\n/** デフォルトのMIDIオクターブ（C4 = MIDI 60） */ const DEFAULT_MIDI_OCTAVE = 60;\n/**\n * ピッチクラスを0-11の範囲に正規化\n */ function mod12(pc) {\n    return (pc % 12 + 12) % 12;\n}\n/**\n * ピッチクラス番号を音名に変換\n */ function pcToName(pc) {\n    return PC_NAMES[mod12(pc)];\n}\n/**\n * 音名をピッチクラス番号に変換\n */ function nameToPc(name) {\n    const index = PC_NAMES.indexOf(name);\n    return index >= 0 ? index : -1;\n}\n\n\n;\n    // Wrapped in an IIFE to avoid polluting the global scope\n    ;\n    (function () {\n        var _a, _b;\n        // Legacy CSS implementations will `eval` browser code in a Node.js context\n        // to extract CSS. For backwards compatibility, we need to check we're in a\n        // browser context before continuing.\n        if (typeof self !== 'undefined' &&\n            // AMP / No-JS mode does not inject these helpers:\n            '$RefreshHelpers$' in self) {\n            // @ts-ignore __webpack_module__ is global\n            var currentExports = module.exports;\n            // @ts-ignore __webpack_module__ is globa